<ul>
  <% @cats.each do |cat| %>
    <li> <%= cat.name %> </li>
    <li> <%= cat.birth_date %> </li>
    <li> <%= cat.color %> </li>
    <li> <%= cat.description %> </li>
    <li> <%= cat.age %> </li>
    <li> <%= cat.sex %> </li>
    <li><a href="cat_url(cat)">Look at me!</a></li>
  <% end %>
</ul>
